; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Cotuber"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/image="res://others/splash.png"
config/icon="res://others/icon.svg"

[audio]

buses/default_bus_layout="res://others/audio_bus.tres"

[autoload]

BackgroundColor="*res://singletons/background_color/background_color.tscn"

[display]

window/size/transparent=true
window/per_pixel_transparency/allowed=true

[file_customization]

folder_colors={
"res://components/": "green",
"res://export/": "orange",
"res://others/": "yellow",
"res://singletons/": "pink",
"res://windows/": "blue"
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
